{
  "name": "rn-tourguide-enhanced",
  "version": "3.5.1",
  "description": "Make an interactive step by step tour guide for your react-native app (a rewrite of react-native-copilot)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "private": false,
  "scripts": {
    "start": "react-native start",
    "test": "TZ=Europe/Paris jest --no-watchman --testPathIgnorePatterns=example-app",
    "tsc": "node_modules/.bin/tsc --noEmit",
    "tsc:watch": "node_modules/.bin/tsc --noEmit --watch",
    "build": "rm -rf lib/ && node_modules/.bin/tsc",
    "config:dev": "json -I -f package.json -e 'this.main=\"index.js\"'",
    "config:npm": "json -I -f package.json -e 'this.main=\"lib/index.js\"'",
    "lint": "eslint src/ --ext .ts,.tsx",
    "verify": "yarn test && yarn tsc && yarn build",
    "prepublishOnly": "yarn verify && yarn config:npm",
    "postpublish": "yarn config:dev",
    "cleaning": "rm -rf node_modules && rm -f yarn.lock && yarn cache clean && yarn install && watchman watch-del-all && rm -fr $TMPDIR/react-*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/puppetmaster886/rn-tourguide-enhanced.git"
  },
  "keywords": [
    "react-native",
    "react-native-joyride",
    "react",
    "copilot",
    "intro.js",
    "intro"
  ],
  "author": "Xavier Carpentier <xcapetir@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/puppetmaster886/rn-tourguide-enhanced/issues"
  },
  "homepage": "https://github.com/puppetmaster886/rn-tourguide-enhanced#readme",
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.74.0",
    "@react-native/eslint-config": "^0.74.0",
    "@react-native/metro-config": "^0.74.0",
    "@tsconfig/react-native": "^3.0.7",
    "@types/hoist-non-react-statics": "3.3.1",
    "@types/jest": "^29.5.0",
    "@types/lodash.clamp": "4.0.6",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "@types/react-test-renderer": "^18.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "json": "^11.0.0",
    "prettier": "^2.8.8",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.74.7",
    "react-native-svg": "^15.13.0",
    "react-native-web": "^0.19.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.0.4"
  },
  "jest": {
    "preset": "react-native",
    "globals": {
      "__TEST__": true
    }
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0",
    "react-native-svg": ">=12.0.0 <16.0.0"
  },
  "dependencies": {
    "flubber": "~0.4.2",
    "hoist-non-react-statics": "~3.0.1",
    "lodash.clamp": "~4.0.3",
    "memoize-one": "5.1.1",
    "mitt": "~1.1.3",
    "react-native-leader-line": "^1.6.0"
  },
  "resolutions": {
    "logkitty": "^0.7.1",
    "mem": "^4.0.0",
    "set-value": "^4.0.1",
    "@types/react": "^18.2.6",
    "@types/react-native": "^0.72.0"
  },
  "engines": {
    "node": ">=18"
  }
}
